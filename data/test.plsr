func main(input: [Int; 4096]) -> (output: [Int; 4096]) {
    for row in 1 ..< 63 {
        for col in 1 ..< 63 {
            let col_start = row * 64
            ---
            let index = col_start + col
            output[index] = input[index]
        }
    }
}

func test() -> (out: Int) {
    for i in 0 ..< 10 {
        out = 1
        ---
        out = 2
    }
}
